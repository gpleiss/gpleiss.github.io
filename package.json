{
  "name": "geoffpleiss.com",
  "author": "Geoff Pleiss <gpleiss@gmail.com>",
  "homepage": "http://geoffpleiss.com/",
  "version": "0.1.0",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-brands": "^5.0.6",
    "@fortawesome/fontawesome-free-solid": "^5.0.6",
    "@fortawesome/react-fontawesome": "0.0.17",
    "bootstrap": "^4.0.0",
    "gh-pages": "^1.1.0",
    "highlight.js": "^9.12.0",
    "katex": "^0.9.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-popper": "^0.8.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-snapshot": "^1.3.0",
    "react-transition-group": "^1.2.1",
    "reactstrap": "^5.0.0-beta"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/sass --include-path ./node_modules src/sass -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/sass --include-path ./node_modules src/sass -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js && react-snapshot",
    "predeploy": "npm run build",
    "deploy": "./node_modules/gh-pages/bin/gh-pages -b master -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "reactSnapshot": {
    "include": [
      "/index.html",
      "/nn_calibration/index.html"
    ],
    "snapshotDelay": 300
  }
}
