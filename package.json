{
  "name": "geoffpleiss.com",
  "author": "Geoff Pleiss <gpleiss@gmail.com>",
  "homepage": "http://geoffpleiss.com/",
  "version": "0.1.0",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.17",
    "bootstrap": "^4.4.1",
    "highlight.js": "^9.18.1",
    "katex": "^0.9.0",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-popper": "^0.8.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.0",
    "react-snapshot": "^1.3.0",
    "react-transition-group": "^1.2.1",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/sass --include-path ./node_modules src/sass -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/sass --include-path ./node_modules src/sass -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js && react-snapshot",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "reactSnapshot": {
    "include": [
      "/index.html",
      "/nn_calibration/index.html"
    ],
    "snapshotDelay": 300
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  }
}
